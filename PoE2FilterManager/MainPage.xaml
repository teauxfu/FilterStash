<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PoE2FilterManager"
             xmlns:ui="clr-namespace:PoE2FilterManagerUI;assembly=PoE2FilterManager.UI"
             x:Class="PoE2FilterManager.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type ui:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
